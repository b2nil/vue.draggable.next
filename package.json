{
  "name": "vuedraggable",
  "version": "4.1.0",
  "description": "draggable component for vue",
  "license": "MIT",
  "main": "dist/vuedraggable.umd.min.js",
  "types": "types/vuedraggable.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/SortableJS/Vue.Draggable.git"
  },
  "private": false,
  "scripts": {
    "dev": "vite example",
    "build": "esbuild src/vuedraggable.js --bundle --sourcemap --external:vue --outfile=dist/vuedraggable.umd.js ",
    "minify": "esbuild src/vuedraggable.js --bundle --minify --sourcemap --external:vue --outfile=dist/vuedraggable.umd.min.js ",
    "prepublishOnly": "npm run lint && npm run test:unit && npm run build:doc && npm run build:type && npm run build",
    "test:unit": "jest --coverage",
    "test:coverage": "jest --coverage --verbose && codecov",
    "build:type": "tsc"
  },
  "keywords": [
    "vue",
    "vuejs",
    "drag",
    "and",
    "drop",
    "list",
    "Sortable.js",
    "component",
    "nested"
  ],
  "dependencies": {
    "sortablejs": "1.15.0"
  },
  "peerDependencies": {
    "vue": "^3.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@swc/core": "^1.2.197",
    "@swc/jest": "^0.2.21",
    "@types/sortablejs": "^1.10.7",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/compiler-sfc": "^3.2.0",
    "@vue/server-renderer": "^3.2.0",
    "@vue/test-utils": "^2.0.0",
    "@vue/vue3-jest": "^28.0.0",
    "babel-jest": "^28.0.0",
    "bootstrap": "^4.3.1",
    "codecov": "^3.2.0",
    "element-plus": "^2.0.0",
    "esbuild": "^0.14.42",
    "eslint": "^6.7.2",
    "font-awesome": "^4.7.0",
    "jest": "^28.0.0",
    "jest-environment-jsdom": "^28.1.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1",
    "typescript": "^4.3.5",
    "vite": "^2.9.10",
    "vue": "^3.2.0",
    "vue-router": "^4.0.0",
    "vuex": "^4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "files": [
    "dist/*.css",
    "dist/*.map",
    "dist/*.js",
    "types/*",
    "src/*"
  ],
  "module": "dist/vuedraggable.umd.js"
}